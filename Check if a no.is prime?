
// Check whether a number is prime or not.
#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    int i,c=0;
    if(n==1)
    cout<<"Not prime number";
    else{
    for(i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            c++;
            break;
        }
    }
    if(c==0)
    cout<<"Prime number";
    else
    cout<<"Not prime number";
    }
    return 0;
}

